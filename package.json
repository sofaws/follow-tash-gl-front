{
  "name": "bigbrother",
  "version": "1.0.0",
  "main": "dist/index.js",
  "description": "Suivi des temps sur Gitlab",
  "private": false,
  "scripts": {
    "start:front": "react-scripts start",
    "start:server": "node server/api.js",
    "start:cron": "node ./server/cron.js",
    "dev:server": "nodemon ./server/cron.js",
    "build": "react-scripts build && cp build/index.html build/404.html",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "lint:check": "eslint . --ext=js,jsx;  exit 0",
    "lint:fix": "eslint . --ext=js,jsx --fix;  exit 0",
    "build-package": "babel src --out-dir dist",
    "postinstall": "babel ./node_modules/ky/index.js --out-file ./node_modules/ky/index.js --config-file=./.babelrc",
    "flow": "flow"
  },
  "dependencies": {
    "@koa/cors": "2",
    "@material-ui/core": "3.9.2",
    "@material-ui/icons": "3.0.1",
    "chartist": "0.10.1",
    "classnames": "2.2.6",
    "date-fns": "1.29.0",
    "dotenv": "6.1.0",
    "flow-bin": "0.89.0",
    "gl-got": "9.0.2",
    "got": "9.3.2",
    "koa": "2.6.2",
    "koa-bodyparser": "4.2.1",
    "koa-router": "7.4.0",
    "koache": "1.0.2",
    "ky": "0.5.1",
    "lowdb": "1.0.0",
    "moment": "^2.24.0",
    "node-cache": "4.2.0",
    "node-cron": "2.0.3",
    "perfect-scrollbar": "1.4.0",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-loading-overlay": "1.0.1",
    "react-markdown": "4.0.6",
    "react-redux": "6.0.0",
    "react-router-dom": "4.3.1",
    "react-scripts": "2.0.3",
    "redux": "4.0.1",
    "redux-devtools-extension": "2.13.7",
    "redux-logger": "3.0.6",
    "redux-saga": "0.16.2",
    "reselect": "4.0.0",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-plugin-import-rename": "1.0.1",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-preset-es2015": "6.24.1",
    "concurrently": "4.1.0",
    "eslint": "5.9.0",
    "eslint-config-prettier": "4.0.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-flowtype": "3.2.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-node": "8.0.0",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-standard": "4.0.0",
    "jest": "23.6.0"
  },
  "optionalDependencies": {
    "prettier": "1.14.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
